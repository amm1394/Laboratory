          <value>EnglishReporter,System.Boolean</value>
          <value>DeliveryDate,System.String</value>
          <value>ContractNo,System.Int32</value>
          <value>Recieptionast,System.String</value>
          <value>Referred,System.String</value>
          <value>LetterDate,System.String</value>
          <value>LetterNo,System.String</value>
          <value>total,System.Decimal</value>
          <value>Executers,System.String</value>
          <value>Tax,System.Int32</value>
          <value>TestForThePresence,System.Byte</value>
          <value>CmpanyAddress,System.String</value>
          <value>DisCount,System.Int32</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Contracts_Info</Name>
        <NameInSource>DataConnection</NameInSource>
        <Parameters isList="true" count="1">
          <value>_x0040_Code,,23,0</value>
        </Parameters>
        <SqlCommand>select Company_Invoice.CompanyName as CompanyName_Invoice , Company_Invoice.EconomicCode,dbo.DateConvertor(dbo.Contracts_Base.ReceptionDate) as ReceptionDate,
	Contracts_Base.Description , Contracts_Base.Uncertainty , Contracts_Base.EnglishReporter ,
	dbo.DateConvertor(dbo.Contracts_Base.Delivery) as DeliveryDate ,Contracts_Base.ContractNo ,
	dbo.Get_User_Name(Contracts_Base.UserID) As Recieptionast, Contracts_Base.Referred ,dbo.DateConvertor(dbo.Contracts_Base.LetterDate) as LetterDate, Contracts_Base.LetterNo ,
	cast(sum(vTotal.Total) as Decimal) as total ,cast(sum(vTotal.DisCount) as bigint) as DisCount, dbo.Get_User_Name(Contracts_Base.Executers) As Executers,vTotal.Tax,Contracts_Base.TestForThePresence , Company_Invoice.CmpanyAddress 
	from Contracts_Base
	left join Companies_Base Company_Invoice on Company_Invoice.CompaniesCode = Contracts_Base.Company_Invoice
    left join dbo.vTotal  on vTotal.ContractsCode = Contracts_Base.ContractsCode  
	where Contracts_Base.ContractsCode = @Code
group by CompanyName,EconomicCode,ReceptionDate,Description,Uncertainty,EnglishReporter,Delivery,ContractNo,Contracts_Base.UserID,Referred,LetterDate,LetterNo,Executers,TestForThePresence,Tax,CmpanyAddress</SqlCommand>
      </Contracts_Info>
      <Parts_Info Ref="4" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
