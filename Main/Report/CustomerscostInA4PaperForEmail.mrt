      <DetailOfContract Ref="8" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>DetailOfContract</Alias>
        <Columns isList="true" count="6">
          <value>ContractNo,System.Int32</value>
