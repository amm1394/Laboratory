        <NameInSource>DataConnection</NameInSource>
        <Parameters isList="true" count="1">
          <value>_x0040_Code,,14,0</value>
        </Parameters>
        <SqlCommand>select Companies_Base.CompanyName , Companies_Base.CmpanyAddress , Companies_Base.EconomicCode, Companies_Base.NationalArgument ,Companies_Base.NationalCode
	,dbo.DateConvertor(Contracts_Base.LetterDate ) as LetterDate , Contracts_Base.LetterNo ,Companies_Base.SetNum, 
	isnull(Contracts_Base.ExpertCompany,'') ExpertCompany  , Contracts_Base.ContractNo , Companies_Base.PostCode ,Companies_Agency.AgencyName ,
	dbo.DateConvertorWithTime(MasterBills.BillDate) AS Date, Companies_Base.FinancialCode1 FinancialCode,Companies_Base.NationalCode
	,Companies_PhoneNum.PhoneNum,Companies_MobileNum.MobileNum
	,case when Companies_Cooperation.CooperationCode in ('BF6434F1-807B-40DB-8B96-3EA4A65671F0','D04DDB41-9B90-4D58-ABE4-405705523A48','969B271E-0392-4A25-99C0-486B3115F336','8F655027-8187-412E-9CED-49433CA62A3D','D7AD3470-063B-4472-A859-892DD77891BB') 
	then 'نقدی' else CooperationName end CooperationName
	from  Contracts_Base
	left join Companies_Base on Companies_Base.CompaniesCode = Contracts_Base.Company_Invoice 
	left join dbo.Companies_Cooperation on Companies_Base.CooperationCode = Companies_Cooperation.CooperationCode 
left join Companies_FaxNum on Companies_FaxNum.CompanyCode = Companies_Base.CompaniesCode 
left join dbo.Companies_Agency on Contracts_Base.OfficesCode = Companies_Agency.AgencyCode
LEFT JOIN dbo.MasterBills ON MasterBills.ContractCode = Contracts_Base.ContractsCode
LEFT JOIN dbo.Companies_PhoneNum ON Companies_PhoneNum.CompanyCode = Contracts_Base.Company_Invoice
LEFT JOIN dbo.Companies_MobileNum ON Companies_MobileNum.CompanyCode = Contracts_Base.Company_Invoice
where  Contracts_Base.ContractsCode =@Code  and MasterBills.IsVoid = 0</SqlCommand>
      </Contract>
