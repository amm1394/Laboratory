        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Contracts_Info</Name>
        <NameInSource>DataConnection</NameInSource>
        <Parameters isList="true" count="1">
          <value>_x0040_Code,,23,0</value>
        </Parameters>
        <SqlCommand>select Company_Invoice.CompanyName as CompanyName_Invoice , Company_Report.CompanyName as CompanyName_Report, Company_Invoice.EconomicCode,dbo.DateConvertor(dbo.Contracts_Base_DeclareCost.ReceptionDate) as ReceptionDate,
	Contracts_Base_DeclareCost.Description , Contracts_Base_DeclareCost.Uncertainty , Contracts_Base_DeclareCost.EnglishReporter , AgencyName,
	dbo.DateConvertor(dbo.Contracts_Base_DeclareCost.Delivery) as DeliveryDate ,Contracts_Base_DeclareCost.ContractNo ,  dbo.SampleSendingType.Name AS SampleSending
	,dbo.Get_User_Name(Contracts_Base_DeclareCost.UserID) As Recieptionast, Contracts_Base_DeclareCost.Referred ,dbo.DateConvertor(dbo.Contracts_Base_DeclareCost.LetterDate) as LetterDate, Contracts_Base_DeclareCost.LetterNo , ReportSendingType.Name , 
	Companies_MobileNum.MobileNum , cast(vTotal.Total as Decimal) as total , dbo.Get_User_Name(Contracts_Base_DeclareCost.Executers) As Executers,vTotal.Tax
	,Contracts_Base_DeclareCost.TestForThePresence , Company_Invoice.CmpanyAddress ,Contracts_Base_DeclareCost.MobileNo ,Contracts_Base_DeclareCost.InsertPicRecieption
	,Contracts_Base_DeclareCost.Marking,ConnectorOfCompanies.UserName as ConnectorName
	from Contracts_Base_DeclareCost
	left join Companies_Base Company_Invoice on Company_Invoice.CompaniesCode = Contracts_Base_DeclareCost.Company_Invoice
	left join Companies_Base Company_Report on Company_Report.CompaniesCode = Contracts_Base_DeclareCost.Company_Report
	left join ConnectorOfCompanies on ConnectorOfCompanies.UserId = Company_Invoice.ConnectorId
left join SampleSendingType on Contracts_Base_DeclareCost.SampleSendingTypeCode = SampleSendingType.Code                 
LEFT JOIN ReportSendingType ON ReportSendingType.Code = Contracts_Base_DeclareCost.ReportSendingTypeCode
LEFT JOIN Companies_MobileNum on Company_Invoice.CompaniesCode = Companies_MobileNum.CompanyCode
left join dbo.vTotal  on vTotal.ContractsCode = Contracts_Base_DeclareCost.ContractsCode  
left join dbo.Companies_Agency on Contracts_Base_DeclareCost.OfficesCode = Companies_Agency.AgencyCode
where Contracts_Base_DeclareCost.ContractsCode = @Code</SqlCommand>
      </Contracts_Info>
