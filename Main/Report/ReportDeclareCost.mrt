        </Parameters>
        <SqlCommand>select MasterTest.MasterName,Parts_Tests_Master_DeclareCost.NumberOfTests ,Parts_Master_DeclareCost.Parts_Master_Code,Parts_Tests_Master_DeclareCost.InvoicePrice,isnull(NumberOfTests * InvoicePrice,0) as TotalPrice
	,case MainTest.CarriedoutintheBonyad when 1 then '' else N'آزمون توسط پیمانکار فرعی انجام می شود'end as CarriedoutintheBonyad
	from Parts_Tests_Master_DeclareCost
	left join MasterTest on MasterTest.MasterTestCode = Parts_Tests_Master_DeclareCost.MasterTestCode
	left join MainTest on MainTest.MainCode = MasterTest.MainTestCode
	left join Parts_Master_DeclareCost on Parts_Master_DeclareCost.Parts_Master_Code = Parts_Tests_Master_DeclareCost.PartsMasterCode
where Parts_Master_DeclareCost.ContractsBaseCode = @Code and Parts_Tests_Master_DeclareCost.[Status] = 1</SqlCommand>
