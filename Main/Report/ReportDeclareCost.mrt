        </Parameters>
        <SqlCommand>select Parts_Master_DeclareCost.Name PiecesName,Parts_Master_DeclareCost.Englishname ,Parts_Master_DeclareCost.PartNumber , Parts_Master_DeclareCost.FeBase ,Yes_No.YesNo ,Parts_Master_DeclareCost.Parts_Master_Code
,Parts_Master_DeclareCost.IsLarge , Parts_Master_DeclareCost.Destruct,Parts_Master_DeclareCost.TestLocation , Contracts_Base_DeclareCost.[immediate] , Contracts_Base_DeclareCost.StepByStep
,dbo.ResizePicture(Picture,48) as SmallPicture
from Parts_Master_DeclareCost inner join Yes_No on Yes_No.Code = Parts_Master_DeclareCost.FeBase
Left Join [Parts_Picture] on [Parts_Picture].Parts_Picture_Code = Parts_Master_DeclareCost.Parts_Master_Code 
inner join Contracts_Base_DeclareCost on Contracts_Base_DeclareCost.ContractsCode = Parts_Master_DeclareCost.ContractsBaseCode
where Parts_Master_DeclareCost.ContractsBaseCode = @Code and Parts_Master_DeclareCost.Void = 0</SqlCommand>
