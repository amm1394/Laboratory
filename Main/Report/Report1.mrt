        <Name>Parts_Info</Name>
        <NameInSource>DataConnection</NameInSource>
        <Parameters isList="true" count="1">
          <value>_x0040_Code,,23,0</value>
        </Parameters>
        <SqlCommand>select Parts_Master.Name PiecesName,Parts_Master.Englishname ,Parts_Master.PartNumber , Parts_Master.FeBase ,Yes_No.YesNo ,Parts_Master.Parts_Master_Code
,Parts_Master.IsLarge , Parts_Master.Destruct,Parts_Master.TestLocation , Contracts_Base.[immediate] , Parts_Master.StepByStep,Parts_Master.TestForThePresence
,Parts_Master.Marking,Parts_Master.NumberOfPices,Parts_Master.NumberOfDetail,Parts_Master.DeskDetail
,dbo.ResizePicture(Picture,48) as SmallPicture
from Parts_Master inner join Yes_No on Yes_No.Code = Parts_Master.FeBase
Left Join [Parts_Picture] on [Parts_Picture].Parts_Picture_Code = [Parts_Master].Parts_Master_Code 
inner join Contracts_Base on Contracts_Base.ContractsCode = Parts_Master.ContractsBaseCode
where Parts_Master.ContractsBaseCode = @Code and Parts_Master.Void = 0</SqlCommand>
      </Parts_Info>
      <Test_Info Ref="5" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>Test_Info</Alias>
        <Columns isList="true" count="7">
          <value>MasterName,System.String</value>
          <value>NumberOfTests,System.Int32</value>
          <value>Parts_Master_Code,System.Guid</value>
          <value>InvoicePrice,System.Int32</value>
          <value>TotalPrice,System.Int32</value>
          <value>CarriedoutintheBonyad,System.String</value>
          <value>LaboratoryName,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Test_Info</Name>
        <NameInSource>DataConnection</NameInSource>
        <Parameters isList="true" count="1">
          <value>_x0040_Code,,23,0</value>
        </Parameters>
        <SqlCommand>select Laboratory.LaboratoryName,MasterTest.MasterName,Parts_Tests_Master.NumberOfTests ,Parts_Master.Parts_Master_Code,Parts_Tests_Master.InvoicePrice,isnull(NumberOfTests * InvoicePrice,0) as TotalPrice
	,case MasterTest.CarriedoutintheBonyad when 1 then '' else N'آزمون توسط پیمانکار فرعی انجام می شود'end as CarriedoutintheBonyad
	from Parts_Tests_Master
	left join MasterTest on MasterTest.MasterTestCode = Parts_Tests_Master.MasterTestCode
	left join MainTest on MainTest.MainCode = MasterTest.MainTestCode
	left join Parts_Master on Parts_Master.Parts_Master_Code = Parts_Tests_Master.PartsMasterCode
	left join Laboratory on Laboratory.LaboratoryCode = MainTest.LaboratoryCode_MainTest
where Parts_Master.ContractsBaseCode = @Code and Parts_Tests_Master.[Status] = 1
</SqlCommand>
