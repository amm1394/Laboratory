          <value>HowToPay,System.String</value>
          <value>ReceiptDate,System.String</value>
          <value>CompanyName,System.String</value>
          <value>Contracts_No,System.String</value>
          <value>AgencyName,System.String</value>
          <value>DayDate,System.String</value>
          <value>FinancialCode,System.Int64</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Receipt</Name>
        <NameInSource>DataConnection</NameInSource>
        <Parameters isList="true" count="1">
          <value>_x0040_ReceiptAmountMasterCode,,14,0</value>
        </Parameters>
        <SqlCommand>select ReceiptAmount_Master.ReceiptNo ,ReceiptAmount_Master.Amount,dbo.Num_ToWords(ReceiptAmount_Master.Amount)as AmountLetter ,
	Receipt_Type.Name as HowToPay,dbo.DateConvertor(ReceiptAmount_Master.ReceiptDate) as ReceiptDate,
	Companies_Base.CompanyName , dbo.Concat_Lab_Name((select ContractNo from dbo.Receiptoffunds_View where ReceiptMasterCode = ReceiptAmount_Master.ReceiptAmountMasterCode FOR XML PATH ('')), 'ContractNo') as Contracts_No
,dbo.Concat_Lab_Name((select Top 1 AgencyName from dbo.Receiptoffunds_View where ReceiptMasterCode = ReceiptAmount_Master.ReceiptAmountMasterCode FOR XML PATH ('')), 'AgencyName') as AgencyName
   , dbo.DateConvertor(ReceiptAmount_Master.ReceiptDate) as DayDate  , Companies_Base.FinancialCode1 FinancialCode
   from ReceiptAmount_Master
left join Companies_Base on ReceiptAmount_Master.CompaniesCode = Companies_Base.CompaniesCode
left join Receipt_Type on Receipt_Type.ReceieptCode = ReceiptAmount_Master.HowToPay
where ReceiptAmount_Master.ReceiptAmountMasterCode =@ReceiptAmountMasterCode
</SqlCommand>
      </Receipt>
      <DetailOfRecieipt Ref="5" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>DetailOfRecieipt</Alias>
        <Columns isList="true" count="6">
          <value>ContractNo,System.Int32</value>
          <value>LetterNo,System.String</value>
          <value>LetterDate,System.String</value>
          <value>Debtor,System.Int32</value>
          <value>Amount,System.Int32</value>
          <value>creditor,System.Int32</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>DetailOfRecieipt</Name>
        <NameInSource>DataConnection</NameInSource>
        <Parameters isList="true" count="1">
          <value>_x0040_ReceiptAmountMasterCode,,14,0</value>
        </Parameters>
        <SqlCommand>select Contracts_Base.ContractNo,Contracts_Base.LetterNo,case when Contracts_Base.LetterDate is null then '' else   Dbo.DateConvertor(Contracts_Base.LetterDate) end as LetterDate,Contracts_RemindForReceipt_View.Debtor,Contracts_RemindForReceipt_View.creditor,sum(ReceiptAmount_Detail.Amount) Amount
from ReceiptAmount_Detail 
left join Contracts_Base on Contracts_Base.ContractsCode = ReceiptAmount_Detail.ContractsBaseCode
left join Contracts_RemindForReceipt_View on Contracts_RemindForReceipt_View.ContractsCode = ReceiptAmount_Detail.ContractsBaseCode
left join ReceiptAmount_Master on ReceiptAmount_Master.ReceiptAmountMasterCode = ReceiptAmount_Detail.ReceiptMasterCode
where ReceiptAmount_Detail.ReceiptMasterCode =@ReceiptAmountMasterCode
and Contracts_Base.Void = 0 and  ReceiptAmount_Master.Void = 0
group by Contracts_Base.ContractNo,Contracts_RemindForReceipt_View.Debtor,Contracts_RemindForReceipt_View.creditor,LetterDate,LetterNo </SqlCommand>
      </DetailOfRecieipt>
