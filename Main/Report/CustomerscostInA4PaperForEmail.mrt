          <value>creditor,System.Int64</value>
          <value>Amount,System.Int32</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>DetailOfContract</Name>
        <NameInSource>DataConnection</NameInSource>
        <Parameters isList="true" count="1">
          <value>_x0040_Code,,14,0</value>
        </Parameters>
        <SqlCommand>select Contracts_Base.ContractNo,Contracts_Base.LetterNo,case when Contracts_Base.LetterDate is null then '' else  dbo.DateConvertor(Contracts_Base.LetterDate)end as LetterDate
,Contracts_RemindForReceipt_View.Debtor,Contracts_RemindForReceipt_View.creditor,sum(ReceiptAmount_Detail.Amount) Amount
from ReceiptAmount_Detail 
left join Contracts_Base on Contracts_Base.ContractsCode = ReceiptAmount_Detail.ContractsBaseCode
left join Contracts_RemindForReceipt_View on Contracts_RemindForReceipt_View.ContractsCode = ReceiptAmount_Detail.ContractsBaseCode
left join ReceiptAmount_Master on ReceiptAmount_Master.ReceiptAmountMasterCode = ReceiptAmount_Detail.ReceiptMasterCode
where ReceiptAmount_Detail.ContractsBaseCode =@Code
and Contracts_Base.Void = 0 and  ReceiptAmount_Master.Void = 0
group by Contracts_Base.ContractNo,Contracts_RemindForReceipt_View.Debtor,Contracts_RemindForReceipt_View.creditor,LetterDate,LetterNo </SqlCommand>
      </DetailOfContract>
