          <value>InsertPicRecieption,System.Boolean</value>
          <value>Marking,System.Boolean</value>
          <value>ConnectorName,System.String</value>
          <value>SabtSamane,System.String</value>
          <value>ExpertCompany,System.String</value>
          <value>CellPhoneExpert,System.String</value>
          <value>NationalCode,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Contracts_Info</Name>
        <NameInSource>DataConnection</NameInSource>
        <Parameters isList="true" count="1">
          <value>_x0040_Code,,23,0</value>
        </Parameters>
        <SqlCommand>select Company_Invoice.CompanyName as CompanyName_Invoice , Company_Report.CompanyName as CompanyName_Report, Company_Invoice.EconomicCode,dbo.DateConvertor(dbo.Contracts_Base.ReceptionDate) as ReceptionDate,
	Contracts_Base.Description , Contracts_Base.Uncertainty , Contracts_Base.EnglishReporter , AgencyName,
	dbo.DateConvertor(dbo.Contracts_Base.Delivery) as DeliveryDate ,Contracts_Base.ContractNo ,  dbo.SampleSendingType.Name AS SampleSending
	,dbo.Get_User_Name(Contracts_Base.UserID) As Recieptionast, Contracts_Base.Referred ,dbo.DateConvertor(dbo.Contracts_Base.LetterDate) as LetterDate, Contracts_Base.LetterNo , ReportSendingType.Name , 
	Companies_MobileNum.MobileNum , cast(vTotal.Total as Decimal) as total , dbo.Get_User_Name(Contracts_Base.Executers) As Executers,vTotal.Tax
	,Contracts_Base.TestForThePresence , Company_Invoice.CmpanyAddress ,Contracts_Base.MobileNo ,Contracts_Base.InsertPicRecieption
	,Contracts_Base.Marking,ConnectorOfCompanies.UserName as ConnectorName,case when Contracts_Base.SabtSamane = 1 then 'بله' else 'خیر' end SabtSamane
,Contracts_Base.ExpertCompany,Contracts_Base.CellPhoneExpert
,case when Company_Invoice.HaghighiOrHoghughiCode = 1 then Company_Invoice.NationalArgument else Company_Invoice.NationalCode end NationalCode
	from Contracts_Base
	left join Companies_Base Company_Invoice on Company_Invoice.CompaniesCode = Contracts_Base.Company_Invoice
	left join Companies_Base Company_Report on Company_Report.CompaniesCode = Contracts_Base.Company_Report
	left join ConnectorOfCompanies on ConnectorOfCompanies.UserId = Company_Invoice.ConnectorId
left join SampleSendingType on Contracts_Base.SampleSendingTypeCode = SampleSendingType.Code                 
LEFT JOIN ReportSendingType ON ReportSendingType.Code = Contracts_Base.ReportSendingTypeCode
LEFT JOIN Companies_MobileNum on Company_Invoice.CompaniesCode = Companies_MobileNum.CompanyCode
left join dbo.vTotal  on vTotal.ContractsCode = Contracts_Base.ContractsCode  
left join dbo.Companies_Agency on Contracts_Base.OfficesCode = Companies_Agency.AgencyCode
where Contracts_Base.ContractsCode = @Code</SqlCommand>
      </Contracts_Info>
