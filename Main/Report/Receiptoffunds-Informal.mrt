        </Parameters>
        <SqlCommand>select ReceiptAmount_Master.ReceiptNo ,ReceiptAmount_Master.Amount,dbo.Num_ToWords(ReceiptAmount_Master.Amount)as AmountLetter ,
	Receipt_Type.Name as HowToPay,dbo.DateConvertor(ReceiptAmount_Master.ReceiptDate) as ReceiptDate,
	Companies_Base.CompanyName , dbo.Concat_Lab_Name((select ContractNo from dbo.Receiptoffunds_View where ReceiptMasterCode = ReceiptAmount_Master.ReceiptAmountMasterCode FOR XML PATH ('')), 'ContractNo') as Contracts_No
,dbo.Concat_Lab_Name((select Top 1 AgencyName from dbo.Receiptoffunds_View where ReceiptMasterCode = ReceiptAmount_Master.ReceiptAmountMasterCode FOR XML PATH ('')), 'AgencyName') as AgencyName
   , dbo.DateConvertor(ReceiptAmount_Master.ReceiptDate) as DayDate  , Companies_Base.FinancialCode
   from ReceiptAmount_Master
left join Companies_Base on ReceiptAmount_Master.CompaniesCode = Companies_Base.CompaniesCode
left join Receipt_Type on Receipt_Type.ReceieptCode = ReceiptAmount_Master.HowToPay
where ReceiptAmount_Master.ReceiptAmountMasterCode = @ReceiptAmountMasterCode
</SqlCommand>
